generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model GeneratedApp {
  id        String   @id @default(nanoid(5))
  model     String
  prompt    String
  code      String
  createdAt DateTime @default(now())

  @@index([id])
}
